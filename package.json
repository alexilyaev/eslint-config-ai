{
  "name": "eslint-config-ai",
  "version": "1.7.0",
  "description": "An opinionated set of ESLint shareable configs by Alex Ilyaev",
  "main": "index.js",
  "scripts": {
    "lint-eslint-deprecated-rules": "eslint-find-rules -d all.js || echo \"ignore failure for now\"",
    "lint-eslint-config-prettier": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "lint": "npm run lint-eslint-config-prettier && npm run lint-eslint-deprecated-rules"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-find-rules": "3.4.0",
    "prettier": "1.18.2"
  },
  "peerDependencies": {
    "babel-eslint": ">=10.0.3",
    "eslint": ">=6.5.1",
    "eslint-config-prettier": ">=6.4.0",
    "prettier": ">=1.18.2"
  },
  "optionalDependencies": {
    "eslint-plugin-compat": ">=2.4.0",
    "eslint-plugin-jest": ">=21.17.0",
    "eslint-plugin-lodash": ">=2.7.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.16.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexilyaev/eslint-config-ai.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Alex Ilyaev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexilyaev/eslint-config-ai/issues"
  },
  "homepage": "https://github.com/alexilyaev/eslint-config-ai#readme"
}
